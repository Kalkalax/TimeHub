             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:models="clr-namespace:TiCloud.Core.Database.Models" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TiCloud_Desktop.views.models"
             xmlns:svg="http://sharpvectors.codeplex.com/svgc/"
             Height="72" Width="380">
    <Grid>



        <Button Style="{DynamicResource UpdateTileButton.xaml}" 
                Click="Button_Click">

            <Button.Content>
                <svg:SvgViewbox Source="/resources/images/database_icon.svg" Width="24" Height="24" />
